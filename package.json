{
  "name": "dspace-angular2-ui",
  "version": "0.0.1",
  "description": "Bootstrapped Webpacked DSpace Angular 2 in TypeScript",
  "main": "server.js",
  "author": {
    "name": "William Welling",
    "email": "wwelling@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wellingWilliam/Angular2DSpaceUIPrototype.git"
  },
  "license": "MIT",
  "contributors": [
    "William Welling <wwelling@outlook.com>"
  ],
  "scripts": {
    "dep": "npm install --global typescript typings webpack nodemon",
    "prebuild": "rimraf dist",
    "build": "webpack",
    "watch": "concurrently \"webpack -w\" \"nodemon dist/server/bundle.js\" \"chrome --disable-web-security --user-data-dir=\"C:\\Google\\Chrome\\Development\" http://localhost:3000\"",
    "test": "concurrently \"nodemon dist/server/bundle.js\" \"chrome --disable-web-security --user-data-dir=\"C:\\Google\\Chrome\\Development\" http://localhost:3000\"",
    "mac-watch": "concurrently \"webpack -w\" \"nodemon dist/server/bundle.js\" \"chrome.sh\"",
    "mac-test": "concurrently \"nodemon dist/server/bundle.js\" \"chrome.sh\"",
    "preinstall": "npm install typings -g",
    "postinstall": "typings install"
  },
  "dependencies": {
    "agentkeepalive": "^2.0.5",
    "angular2": "2.0.0-beta.9",
    "angular2-universal-preview": "~0.70.8",
    "angular2-websocket": "^0.6.2",
    "css": "2.2.1",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.3",
    "es7-reflect-metadata": "^1.6.0",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "parse5": "^1.5.0",
    "preboot": "~1.1.0",
    "rxjs": "^5.0.0-beta.2",
    "xhr2": "^0.1.3",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "ws": "^1.0.1",
    "cors": "^2.7.1",
    "nodemon": "^1.8.1",
    "rimraf": "^2.5.0",
    "typings": "^0.7.8",
    "typescript": "^1.8.5",
    "webpack": "^1.12.10",
    "webpack-merge": "^0.7.1",
    "webpack-dev-server": "^1.14.0",
    "concurrently": "^2.0.0",
    "ts-loader": "^0.8.1",
    "file-loader": "^0.8.5",
    "url-loader": "^0.5.7",
    "resolve-url-loader": "^1.4.3",
    "imports-loader": "^0.6.5",
    "css-loader": "^0.23.1",
    "node-loader": "^0.5.0",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0"
  }
}
